{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-components": "^1.1.16",
    "@ant-design/pro-layout": "^6.5.0",
    "antd": "^4.22.6",
    "axios": "^0.27.2",
    "immutable": "^4.1.0",
    "mobx": "^6.6.1",
    "mobx-persist-store": "^1.0.6",
    "mobx-react": "^7.5.2",
    "react": "17.x",
    "react-dom": "17.x",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-immutable": "^4.0.0",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.1",
    "tslib": "^2.4.0",
    "umi": "^3.5.32",
    "v8-compile-cache": "^2.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/redux-immutable": "^4.0.3",
    "@types/redux-promise": "^0.5.29",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.32",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
